{
  "name": "@braks/vue-flow-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --dir examples/vite dev",
    "docs": "pnpm --dir docs dev",
    "changelog": "git-cliff --output CHANGELOG.md && git add CHANGELOG.md && git commit -m \"chore: Update CHANGELOG.md\"",
    "release": "turbo run release",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "typedocs": "turbo run typedoc"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@changesets/cli": "^2.25.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "shx": "^0.3.4",
    "turbo": "^1.5.5",
    "typescript": "^4.8.4"
  }
}
